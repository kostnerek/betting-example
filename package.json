{
  "name": "betting-example",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/api-gateway/main",
    "start:all": "./scripts/start-all-services.sh",
    "start:all:dev": "concurrently \"nest start api-gateway --watch\" \"nest start bets --watch\" \"nest start odds --watch\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "postproto:gen": "tsc scripts/generate-types-index.ts && node scripts/generate-types-index.js",
    "proto:gen": "./scripts/generate-proto.sh",
    "prisma:generate": "npm run prisma:generate:bets && npm run prisma:generate:odds",
    "prisma:generate:bets": "prisma generate --schema=./prisma/bets/schema.prisma",
    "prisma:generate:odds": "prisma generate --schema=./prisma/odds/schema.prisma",
    "prisma:migrate": "npm run prisma:migrate:bets && npm run prisma:migrate:odds",
    "prisma:migrate:bets": "prisma migrate dev --schema=./prisma/bets/schema.prisma",
    "prisma:migrate:odds": "prisma migrate dev --schema=./prisma/odds/schema.prisma",
    "prisma:migrate:deploy": "npm run prisma:migrate:deploy:bets && npm run prisma:migrate:deploy:odds",
    "prisma:migrate:deploy:bets": "prisma migrate deploy --schema=./prisma/bets/schema.prisma",
    "prisma:migrate:deploy:odds": "prisma migrate deploy --schema=./prisma/odds/schema.prisma",
    "prisma:reset": "npm run prisma:reset:bets && npm run prisma:reset:odds",
    "prisma:reset:bets": "prisma migrate reset --schema=./prisma/bets/schema.prisma",
    "prisma:reset:odds": "prisma migrate reset --schema=./prisma/odds/schema.prisma",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/api-gateway/test/jest-e2e.json",
    "test:e2e:setup": "./scripts/setup-test-db.sh",
    "test:e2e:teardown": "./scripts/teardown-test-db.sh",
    "test:e2e:full": "npm run test:e2e:setup && npm run test:e2e && npm run test:e2e:teardown"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.8.0",
    "@grpc/reflection": "^1.0.4",
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@prisma/client": "6.15.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.58.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "date-fns": "^4.1.0",
    "dinero.js": "^1.9.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "ts-proto": "^2.7.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.5",
    "@types/bcrypt": "^6.0.0",
    "@types/dinero.js": "^1.9.4",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "concurrently": "^9.2.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "prisma": "6.15.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1"
    }
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
